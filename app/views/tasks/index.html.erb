<h1>Tasks#index</h1>
<p>Find me in app/views/tasks/index.html.erb</p>

<table class="table table-striped">
  <thead>
    <tr>
      <th scope="col">id</th>
      <th scope="col">Task</th>
      <th scope="col">Category</th>
    </tr>
  </thead>
  <tbody>
    <% @tasks.each do |task| %>
      <tr>
        <th scope="row"><%= task.id %></th>
        <td><%= task.name %></td>
        <td><%= task.category_id %></td>
      </tr>
    <% end %>
  </tbody>
</div>

